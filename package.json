{
  "name": "zanata-ui",
  "version": "2.0.0-6",
  "description": "The Core UI for Zanata",
  "main": "lib/index.js",
  "jsnext:main": "src/index.js",
  "style": "lib/styles/index.css",
  "scripts": {
    "build:css": "npm run build:sass && mkdir -p lib/styles && postcss -c postcss.config.json",
    "build:icons": "npm run svg && npm run icons",
    "build:lib": "babel src --out-dir lib --stage 0",
    "build:sass": "npm run variables && node-sass src/components/Select/index.scss -o src/components/Select --include-path node_modules --importer node_modules/node-sass-json-importer/dist/node-sass-json-importer.js",
    "build:styleguide": "utils/extractCssComments/.bin/extractCssComments -i src/styles/index.css -o styleguide.md",
    "build": "npm run build:styleguide & npm run build:icons && npm run build:lib && npm run build:css",
    "check": "npm run lint && npm run test",
    "clean:icons": "rimraf src/components/Icons/index.jsx src/components/Icons/icons.svg",
    "clean": "rimraf lib dist & npm run clean:icons",
    "hot-dev-server": "webpack-dev-server -d --config webpack.dev.config.js --progress --inline --hot --port 8000",
    "icons": "node utils/createIconsComponent",
    "lint": "eslint src/**/*.js src/**/*.jsx",
    "postversion": "git push && git push --tags && npm run clean",
    "prepublish": "npm run clean && npm run build",
    "preversion": "npm run clean && npm run check",
    "start": "npm run build:css & npm run build:styleguide & npm run build:icons && npm run hot-dev-server",
    "svg": "svg-sprite -sD src/components/Icons --symbol-dest '' --ss icons.svg --si src/components/Icons/svgs/*.svg",
    "test": "echo \"Tests coming soon\"",
    "variables": "node utils/writeVariablesJson",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zanata/zanata-ui.git"
  },
  "keywords": [
    "zanata",
    "ui",
    "browser",
    "react"
  ],
  "author": {
    "name": "Luke Brooker"
  },
  "license": "MIT",
  "homepage": "https://github.com/zanata/zanata-ui",
  "dependencies": {
    "classnames": "^2.1.3",
    "combokeys": "^2.4.3",
    "dom-helpers": "^2.4.0",
    "fixed-data-table": "^0.5.0",
    "flat": "^2.0.0",
    "lodash": "^4.1.0",
    "normalize": "^0.3.1",
    "normalize.css": "^3.0.3",
    "react": "^0.14.6",
    "react-dom": "^0.14.7",
    "react-input-autosize": "^0.6.8",
    "react-motion": "^0.2.7",
    "react-overlays": "^0.6.0",
    "react-prop-types": "^0.3.0",
    "react-router": "^0.13.3",
    "react-select": "^1.0.0-beta8",
    "shallowequal": "^0.2.2",
    "warning": "^2.1.0"
  },
  "devDependencies": {
    "atomic-loader": "^0.1.3",
    "autoprefixer": "^6.3.1",
    "babel-cli": "^6.4.5",
    "babel-core": "^6.3.26",
    "babel-eslint": "^4.1.8",
    "babel-loader": "^6.2.1",
    "babel-plugin-lodash": "^2.0.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "eslint": "^1.10.3",
    "eslint-config-standard": "^4.4.0",
    "eslint-config-zanata": "^1.0.3",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-no-console-log": "^1.0.0",
    "eslint-plugin-react": "^3.16.1",
    "eslint-plugin-standard": "^1.3.1",
    "extract-text-webpack-plugin": "^0.8.2",
    "faker": "^3.0.1",
    "html-loader": "^0.3.0",
    "jsonfile": "^2.2.3",
    "markdown-loader": "^0.1.7",
    "marked": "^0.3.5",
    "nets": "^3.1.0",
    "node-sass": "^3.4.2",
    "node-sass-json-importer": "^1.0.5",
    "null-loader": "^0.1.1",
    "postcss": "^5.0.14",
    "postcss-browser-reporter": "^0.4.0",
    "postcss-cli": "^2.5.0",
    "postcss-cssnext": "^2.4.0",
    "postcss-import": "^8.0.2",
    "postcss-loader": "^0.8.0",
    "postcss-reporter": "^1.3.1",
    "postcss-safe-parser": "^1.0.4",
    "postcss-url": "^5.1.0",
    "react-hot-loader": "^1.2.8",
    "rimraf": "^2.4.3",
    "standard": "^5.2.2",
    "stats-webpack-plugin": "^0.2.1",
    "style-loader": "^0.13.0",
    "svg-sprite": "^1.2.9",
    "webpack": "^1.12.12",
    "webpack-dev-server": "^1.10.1"
  },
  "gitHead": "de1fc3cae6d75626bf3926148d0b37c79473bd7e",
  "readmeFilename": "README.md",
  "bugs": {
    "url": "https://zanata.atlassian.net/secure/Dashboard.jspa"
  }
}
